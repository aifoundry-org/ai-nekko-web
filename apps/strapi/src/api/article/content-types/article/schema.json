{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "common.seo"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "mappedBy": "articles"
    },
    "date": {
      "type": "date"
    },
    "paragraphs": {
      "type": "component",
      "repeatable": true,
      "component": "article.paragraphs"
    },
    "contentNavigation": {
      "type": "component",
      "repeatable": true,
      "component": "article.content-navigation"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "articles"
    }
  }
}
